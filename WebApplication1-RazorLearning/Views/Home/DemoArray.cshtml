@model WebApplication1_RazorLearning.Models.Product[]

@{
    ViewBag.Title = "DemoArray";
    //Layout = null;
}
@if (Model.Length > 0)
{
    <table> 
        <thead><tr><th>Product</th><th>Price</th></tr></thead>
        <tbody>
            @foreach (WebApplication1_RazorLearning.Models.Product p in Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>$@p.Price</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h2>No product data</h2>
}
